{
  "name": "dlq-utils",
  "version": "1.0.0",
  "description": "A CLI to help with DLQ message processing",
  "scripts": {
    "test": "jest",
    "build": "rm -rf dist && babel src -d dist",
    "lint": "eslint './src/**/*.js'",
    "release": "pkg package.json"
  },
  "bin": "./dist/index.js",
  "keywords": [
    "cli",
    "dlq",
    "dead-letter-queue",
    "aws"
  ],
  "author": "leoaugustov",
  "license": "MIT",
  "pkg": {
    "targets": [
      "node14-macos-x64",
      "node14-linux-x64"
    ],
    "outputPath": "artifacts"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.170.0",
    "@aws-sdk/client-sqs": "^3.76.0",
    "chalk": "^4.1.2",
    "commander": "^9.2.0",
    "n-readlines": "^1.0.1",
    "properties-reader": "^2.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "babel-jest": "^27.5.1",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^27.5.1",
    "jest-extended": "^3.2.0",
    "pkg": "^5.8.0",
    "prettier": "2.6.2",
    "tempy": "^3.0.0",
    "testcontainers": "^9.0.0"
  }
}
